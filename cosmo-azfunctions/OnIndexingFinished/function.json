{
  "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "exampleOutput",
      "type": "cosmosDB",
      "databaseName": "MyDatabase",
      "collectionName": "MyCollection",
      "createIfNotExists": true,
      "connectionStringSetting": "COSMOSDB_CONNECTION",
      "direction": "out"
  }
  ],
  "scriptFile": "../dist/OnIndexingFinished/index.js"
}